      "auth": {
        "type": "none"
      }
    },
    {
      "name": "users (Copy)",
      "path_prefix": "/users",
      "methods": [
        "GET",
        "POST",
        "PUT"
      ],
      "upstreams": [
        "http://localhost:9001",
        "http://localhost:9003",
        "http://localhost:9004"
      ],
      "enabled": true,
      "auth": {},
      "filters": [
        {
          "name": "Logging",
          "settings": {
            "format": "json",
            "level": "INFO",
            "output": "stdout"
          }
        },
        {
          "name": "SetPath",
          "settings": {
            "path": "/v4"
          }
        },
        {
          "name": "RequestSize",
          "settings": {
            "max_size": 10
          }
        }
      ],
      "lb": {
        "strategy": "round-robin",
        "targets": [
          "http://localhost:9001",
          "http://localhost:9003",
          "http://localhost:9004"
        ],
        "state": {
          "http://localhost:9001": {
            "healthy": true,
            "failCount": 0,
            "test": false,
            "lastFalure": null
          },
          "http://localhost:9003": {
            "healthy": true,
            "failCount": 0,
            "test": false,
            "lastFalure": null
          },
          "http://localhost:9004": {
            "healthy": true,
            "failCount": 0,
            "test": false,
            "lastFalure": null
          }
        },
        "cooldown": 10,
        "failureCount": 3
      }
    },
